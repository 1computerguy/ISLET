#!/usr/bin/env bash
# Global Variables
CONFIG=LOCATION
if [ -f $CONFIG ]; then
	source $CONFIG
else
	echo "$CONFIG not found! Exiting..."
	exit 1
fi

clear

echo "Welcome to Zookeeper!"
echo "====================="
cat <<"EOF"
==============================================

   Zoo Keeper: A Linux-based Training System

   Web: https://github.com/jonschipp/zookeeper

==============================================
EOF
echo
echo "A place to try out Linux software."
echo

#########################################
exec timeout 1m $LAUNCH_CONTAINER $IMAGE
#########################################
